<div id="m-8-configuracion-basica" v-cloak>

  <modulo-contenedor-curso :navegar-atras="navegarAtras" :navegar-siguiente="navegarSiguiente" :breadcrumb="breadcrumb"
    :usuario="usuario" :curso="curso" :objeto-seleccionado.sync="objetoSeleccionado"
    :titulo-temporal.sync="tituloEvaluacion" @evaluacion-individual="evaluacionIndividual" ref="curso"
    :mostrar-icono-repetir="mostrarIconoRepetir" @intentar-nuevamente="intentarNuevamente" :progreso="progreso"
    @click-asistente-buho="clickMostrarPista">
    <!--mostrarIconoRepetir es una varible de tipo Boolean-->

    <!-- si el contenido retornado es un submodulo se permiten las evaluaciones -->
    <modulo-ev-individual v-if="evIndividual" :submodulo="objetoSeleccionado" :usuario="usuario"
      @finaliza-evaluacion="finalizaEvaluacion" ref="componenteEvaluacion" :curso="curso"
      @actualiza-progreso="actualizaProgreso">

    </modulo-ev-individual>
    <div v-else id="contenedor-lienzo">

      <div id="carouselConfiguracion" class="pl-4 carousel slide" data-ride="carousel" data-interval="false"
        data-wrap="false">
        <ol class="carousel-indicators carousel-indicators-numbers">
          <li data-target="#carouselConfiguracion" data-slide-to="0" class="indicador active" @click="obtenerIndice">1
          </li>
          <li data-target="#carouselConfiguracion" data-slide-to="1" class="indicador" @click="obtenerIndice">2</li>
          <li data-target="#carouselConfiguracion" data-slide-to="2" class="indicador" @click="obtenerIndice">3</li>
          <li data-target="#carouselConfiguracion" data-slide-to="3" class="indicador" @click="obtenerIndice">4</li>
          <li data-target="#carouselConfiguracion" data-slide-to="4" class="indicador" @click="obtenerIndice">5</li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-100" src="/images/informaticabasica/modulo8/configuracion_basica/conf0.jpeg"
              alt="Ingresar correo">
          </div>
          <div class="carousel-item ">
            <img class="d-block w-100" src="/images/informaticabasica/modulo8/configuracion_basica/conf1.png"
              alt="Ingresar correo">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/images/informaticabasica/modulo8/configuracion_basica/conf2.png"
              alt="Cuenta agregada">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/images/informaticabasica/modulo8/configuracion_basica/conf3.png"
              alt="Configuracion de fecha y hora">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/images/informaticabasica/modulo8/configuracion_basica/conf4.png"
              alt="MÃ¡s configuraciones">
          </div>


        </div>
        <a class="carousel-control-prev" href="#carouselConfiguracion" role="button" data-slide="prev"
          @click="obtenerIndice">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselConfiguracion" role="button" data-slide="next"
          @click="obtenerIndice">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>

    </div>

  </modulo-contenedor-curso>
  <!--  -->

</div>

<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>

