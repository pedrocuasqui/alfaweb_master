<div id="m-2-gestion-archivos" v-cloak>

  <!-- MODALS******************************************************************************************* -->

  <!-- CONTENIDOS******************************************************************************************* -->

  <modulo-contenedor-curso :navegar-atras="navegarAtras" :navegar-siguiente="navegarSiguiente" :breadcrumb="breadcrumb"
    :usuario="usuario" :curso="curso" :objeto-seleccionado.sync="objetoSeleccionado"
    :titulo-temporal.sync="tituloEvaluacion" @evaluacion-individual="evaluacionIndividual" ref="curso"
    :mostrar-icono-repetir="mostrarIconoRepetir" @intentar-nuevamente="intentarNuevamente" :progreso="progreso"
    @click-asistente-buho="clickMostrarPista">
    <!--mostrarIconoRepetir es una varible de tipo Boolean-->

    <!-- si el contenido retornado es un submodulo se permiten las evaluaciones -->

    <modulo-ev-individual v-if="evIndividual" :submodulo="objetoSeleccionado" :usuario="usuario"
      @finaliza-evaluacion="finalizaEvaluacion" ref="componenteEvaluacion" :curso="curso"
      @actualiza-progreso="actualizaProgreso">

    </modulo-ev-individual>
    <div v-else id="contenedor-lienzo">
      <!-- CONTENIDO  -->


      <div class="row">
        <div class="col-2">
          <!-- CONTROLES -->
          <div class="pl-4 list-group gestion_de_archivos" id="list-tab" role="tablist">
            <a class="list-group-item list-group-item-action active" id="list-crear-list" data-toggle="list"
              href="#list-crear" role="tab" aria-controls="crear" @click="clickReestablecerCarousel('Crear')">
              <p>Crear</p>
            </a>
            <a class="list-group-item list-group-item-action" id="list-renombrar-list" data-toggle="list"
              href="#list-renombrar" role="tab" aria-controls="renombrar"
              @click="clickReestablecerCarousel('Renombrar')">
              <p> Cambiar nombre </p>
            </a>
            <a class="list-group-item list-group-item-action" id="list-copiar-listt" data-toggle="list"
              href="#list-copiar" role="tab" aria-controls="copiar" @click="clickReestablecerCarousel('Copiar')">
              <p>
                Copiar
              </p>
            </a>
            <!-- <a class="list-group-item list-group-item-action" id="list-mover-list" data-toggle="list" href="#list-mover"
              role="tab" aria-controls="mover">Mover</a> -->
            <a class="list-group-item list-group-item-action" id="list-eliminar-list" data-toggle="list"
              href="#list-eliminar" role="tab" aria-controls="eliminar" @click="clickReestablecerCarousel('Eliminar')">
              <p>
                Eliminar
              </p>
            </a>
          </div>
        </div>
        <div class="col-10">
          <div class="tab-content" id="nav-tabContent">





            <!-- CREAR UN ARCHIVO -->
            <div class="tab-pane fade show active" id="list-crear" role="tabpanel" aria-labelledby="list-crear-list">
              <div id="carouselCrear" class="carousel slide" data-ride="carousel" data-interval="false"
                data-wrap="false">
                <ol class="carousel-indicators carousel-indicators-numbers">
                  <li data-target="#carouselCrear" data-slide-to="0" class="indicador active"
                    @click="obtenerIndice('crear')">1
                  </li>
                  <li data-target="#carouselCrear" data-slide-to="1" class="indicador" @click="obtenerIndice('crear')">2
                  </li>
                  <li data-target="#carouselCrear" data-slide-to="2" class="indicador" @click="obtenerIndice('crear')">3
                  </li>
                  <li data-target="#carouselCrear" data-slide-to="3" class="indicador" @click="obtenerIndice('crear')">4
                  </li>
                  <li data-target="#carouselCrear" data-slide-to="4" class="indicador" @click="obtenerIndice('crear')">5
                  </li>

                </ol>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img class="d-block w-100" src="/images/informaticabasica/modulo2/gestion_archivos/crear_1.jpg"
                      alt="Seleccionar un lugar para crear">
                  </div>
                  <div class="carousel-item ">
                    <img class="d-block w-100" src="/images/informaticabasica/modulo2/gestion_archivos/crear_2.jpg"
