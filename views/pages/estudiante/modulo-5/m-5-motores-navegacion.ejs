<div id="m-5-motores-navegacion" v-cloak>


    <!-- CONTENIDOS******************************************************************************************* -->
  
    <modulo-contenedor-curso :navegar-atras="navegarAtras" :navegar-siguiente="navegarSiguiente" :breadcrumb="breadcrumb"
      :usuario="usuario" :curso="curso" :objeto-seleccionado.sync="objetoSeleccionado"
      :titulo-temporal.sync="tituloEvaluacion" @evaluacion-individual="evaluacionIndividual" 
      ref="curso"
      :mostrar-icono-repetir="mostrarIconoRepetir"
      @intentar-nuevamente="intentarNuevamente"
      :progreso="progreso"
      @click-asistente-buho="clickMostrarPista">
      <!--mostrarIconoRepetir es una varible de tipo Boolean-->
      
      <!-- si el contenido retornado es un submodulo se permiten las evaluaciones -->
        <modulo-ev-individual v-if="evIndividual" :submodulo="objetoSeleccionado" :usuario="usuario"
          @finaliza-evaluacion="finalizaEvaluacion" ref="componenteEvaluacion" :curso="curso"
          @actualiza-progreso="actualizaProgreso">
      
      </modulo-ev-individual>
  
      <div v-else>
        <div id="carouselMotores" class="carousel slide" data-ride="carousel" data-interval="false" data-wrap="false">
          <ol class="carousel-indicators carousel-indicators-numbers">
            <li data-target="#carouselMotores" data-slide-to="0" class="indicador active" @click="obtenerIndice">1</li>
            <li data-target="#carouselMotores" data-slide-to="1" class="indicador" @click="obtenerIndice">2</li>
            <li data-target="#carouselMotores" data-slide-to="2" class="indicador" @click="obtenerIndice">3</li>
            <li data-target="#carouselMotores" data-slide-to="3" class="indicador" @click="obtenerIndice">4</li>
            <li data-target="#carouselMotores" data-slide-to="4" class="indicador" @click="obtenerIndice">5</li>
            <li data-target="#carouselMotores" data-slide-to="5" class="indicador" @click="obtenerIndice">6</li>
          </ol>
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="https://www.actualidadgadget.com/wp-content/uploads/2014/07/motores-de-b%C3%BAsqueda-en-IE.jpg" alt="Motores de búsqueda">
            </div>
            <div class="carousel-item ">
              <img class="d-block w-100" src="/images/informaticabasica/modulo5/motores_navegacion/motores1.png" alt="Buscar un motor de búsqueda">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/informaticabasica/modulo5/motores_navegacion/motores2.png" alt="Escribir lo que desea búscar">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/informaticabasica/modulo5/motores_navegacion/motores3.png" alt="Seleccionar una sugerencia o escribir su propio texto">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/informaticabasica/modulo5/motores_navegacion/motores4.png" alt="Seleccionar uno de los resultados">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/informaticabasica/modulo5/motores_navegacion/motores5.png" alt="Visualizar el contenido">
            </div>
  
          </div>
          <a class="carousel-control-prev" href="#carouselMotores" role="button" data-slide="prev"
            @click="obtenerIndice">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselMotores" role="button" data-slide="next"
            @click="obtenerIndice">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
  
      </div>
    </modulo-contenedor-curso>
    <!--  -->
  </div>
  
  <%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
  
  
