                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="/images/informaticabasica/modulo2/gestion_archivos/crear_3.jpg"
                      alt="Seleccionar nuevo ">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="/images/informaticabasica/modulo2/gestion_archivos/crear_4.jpg"
                      alt="Seleccionar carpeta">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="/images/informaticabasica/modulo2/gestion_archivos/crear_5.jpg"
                      alt="Seleccionar carpeta">
                  </div>

                </div>
                <a class="carousel-control-prev" href="#carouselCrear" role="button" data-slide="prev"
                  @click="obtenerIndice('crear')">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselCrear" role="button" data-slide="next"
                  @click="obtenerIndice('crear')">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>
            </div>







